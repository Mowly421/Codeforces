#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,X,Y,Z,X_sum=0,Y_sum=0,Z_sum=0;
    cin>>n;
 
    for(int i=0;i<n;i++)
    {
        cin>>X>>Y>>Z;
        X_sum+=X;
        Y_sum+=Y;
        Z_sum+=Z;
    }
    if(X_sum==0 && Y_sum==0 && Z_sum==0)
    {
        cout<<"YES"<<endl;
    }
    else
    {
        cout<<"NO"<<endl;
    }
}
